var myGoogleMap={map:null,marker:null,homePageMap:function(){var myGoogleMapLatLng={lat:gmapLatitude,lng:gmapLongitude};var isDraggable=$(document).width()>990?true:false;myGoogleMap.map=new google.maps.Map(document.getElementById('google-map-wrapper'),{zoom:gmapZoomLevel+1,center:myGoogleMapLatLng,draggable:isDraggable,mapTypeControl:false,scaleControl:false,scrollwheel:false,panControl:false,streetViewControl:false});var image='https://www.google.com/mapfiles/marker.png';myGoogleMap.marker=new google.maps.Marker({position:myGoogleMapLatLng,map:myGoogleMap.map,icon:image});google.maps.event.addDomListener(window,"resize",function(){var center=myGoogleMap.marker.getPosition();google.maps.event.trigger(myGoogleMap.map,"resize");myGoogleMap.map.setCenter(center);});},resizeMap:function(){var center=myGoogleMap.marker.getPosition();google.maps.event.trigger(myGoogleMap.map,"resize");myGoogleMap.map.setCenter(center);}}
$(document).ready(function(){var distance=$(window).scrollTop();if(distance-scrollAmount>pixelsToDispMap){myGoogleMap.homePageMap();}});$(document).scroll(function(){var distance=$(window).scrollTop();if(distance-scrollAmount>pixelsToDispMap){myGoogleMap.homePageMap();$(this).unbind("scroll");}});